{
  "name": "pamediagroup-qa-tech-assessment",
  "version": "1.0.0",
  "description": "Scaffolding to support PA Media Group's QA Technical Assessments",
  "scripts": {
    "format": "prettier --write '{support,test}/**/*.ts' '**/*.md'",
    "lint:all": "npm run lint:code && npm run lint:package",
    "lint:code": "tslint --project tsconfig.json --config tslint.json",
    "lint:package": "npmPkgJsonLint -c ./.npmpackagejsonlintrc.json .",
    "test": "jest --verbose --runInBand --detectOpenHandles"
  },
  "author": "PA Media Group",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^8.2.0"
  },
  "private": true,
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/supertest": "^2.0.9",
    "axios": "^0.21.1",
    "check-types": "^11.1.2",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "lint": "^0.7.0",
    "lodash": "^4.17.21",
    "npm-package-json-lint": "^5.1.0",
    "prettier": "^1.19.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.3"
  }
}
